.CreateFlag {
  width: 100%;
  height: 100%;
  position: fixed;
  top: 0;
  left: 0;
  background: white;
}

.CreateFlag .title {
  width: 100%;
  text-align: center;
  font-weight: 800;
}

.CreateFlag .text {
  background: #fff;
  width: 85%;
  margin: 30rpx auto;
  border: 1rpx solid #ccc;
  border-radius: 20rpx;
  padding: 20rpx;
  font-size: 32rpx;
}

.close {
  text-align: right;
  padding: 10rpx;
}

.save {
  width: 100%;
  text-align: center;
}

.container {
  width: 100%;
  height: 100%;
}

